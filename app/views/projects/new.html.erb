<h1 align="center">Personal Details</h1>
<%=form_for(@project) do |n|  %>
    Name  <%= n.text_field 'name'%><br /><br />
    Date Of Birth  <%= n.text_field 'dateofbirth'%><br /><br />
    Gender  <%= n.radio_button  :gender,'male',:checked => true %> male
    <%= n.radio_button  :gender ,'female' %>female<br /><br />
    Email  <%= n.text_field 'email'%><br /> <br />
    Address  <%= n.text_field 'address'%><br /> <br />
    City  <%= n.select :city, options_for_select(["Bangalore", "Hyderabad", "Pune", "Delhi", "chennai", "mumbai"])%><br /><br />
    State  <%= n.select :state, options_for_select(["Karnataka", "Ap", "Maharasta", "Tamil Nadu"])%><br /><br />
    Country  <%= n.select :country, options_for_select(["India","Japan", "china", "Iceland", "America"])%><br /><br />
    Mobile Number  <%= n.text_field 'mobile'%><br /><br />
    <%= n.submit %>
<% end %>
